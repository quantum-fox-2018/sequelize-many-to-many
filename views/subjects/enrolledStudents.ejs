<%- include ../partials/header.ejs%>

  <h3>Enrolled Students</h3><br>
  <table>
    <thead>
      <th>Name</th>
      <th>Score</th>
    </thead>
    <tbody>
      <% for(let i=0; i<subjects.students.length; i++){ %>
        <td><%= subjects.students[i].firstName %> <%= subjects.students[i].lastName %></td>
        <% if(subjects.students[i].StudentSubject.score==null){ %>
          <td><a href="/students/<%= subjects.Students[i].StudentSubject.StudentId %>/
            <%= subjects.Students[i].StudentSubject.SubjectId %>/givescore">Give Score</a></td>
        <% } else { %>
          <td><%= subjects.students[i].StudentSubject.score %></td>
        <% } %>
      <% } %>
    </tbody>
  </table>

<%- include ../partials/footer.ejs%>
