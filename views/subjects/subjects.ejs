<%- include ../partials/header.ejs%>
  <h3>Data Subject</h3></br>
  <a href="subjects/add">Add Subject</a></br></br>
  <table>
    <thead>
      <th>No</th>
      <th>ID</th>
      <th>Subject Name</th>
      <th>Teacher</th>
      <th>Actions</th>
    </thead>
    <tbody>
      <% for(let i=0; i<subjects.length; i++){ %>
        <tr>
          <td><%= i+1 %></td>
          <td><%= subjects[i].id %></td>
          <td><%= subjects[i].name %></td>
          <td>
            <% for(let j=0; j<subjects[i].teachers.length; j++){ %>
              <%= subjects[i].teachers[j].firstName %> <%= subjects[i].teachers[j].lastName %><br>
            <% } %>
          </td>
          <td>
            <a href="/subjects/<%= subjects[i].id %>/enrolledStudent">Enrolled Students</a>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
<%- include ../partials/footer.ejs%>
